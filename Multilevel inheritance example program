class student():
  def __init__(self,name,age,rollno):
    self.name = name
    self.age = age
    self.rollno = rollno
class childstudent():
  def __init__(self,name,age,rollno):
    self.name = name
    self.age = age
    self.rollno = rollno 
class childstudent1():
  def __init__(self,name,age,rollno):
    self.name = name
    self.age = age
    self.rollno = rollno
class childstudent2():
  def __init__(self,name,age,rollno):
    self.name = name
    self.age = age
    self.rollno = rollno
stu1 = student("siree",18,12)
stu2 = student("satya",19,13)
print(stu1.name,stu1.age,stu1.rollno)
print(stu2.name,stu1.age,stu1.rollno)
