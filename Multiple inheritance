ass student1():
  def __init__(self,name,age,rollno):
    self.name = name
    self.age = age
    self.rollno = rollno
    
class student2():
  def __init__(self,name,age,rollno,branch):
    self.name = name
    self.age = age
    self.rollno = rollno 
    self.branch = branch
    
class childstudent(student1,student2):
  def __init__(self,name,age,rollno,branch):
    self.name = name
    self.age = age
    self.rollno = rollno 
    self.branch = branch

stu1=student1("Rishi",18,1)
stu2=student2("Aditya",19,2,"AIDS")
print(stu1.name,stu1.age,stu1.rollno)
print(stu2.name,stu2.age,stu2.rollno,stu2.branch)
  
